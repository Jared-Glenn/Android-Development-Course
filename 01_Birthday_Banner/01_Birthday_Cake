fun main() {
    val age = 365 * 5
    val name = "Rover"
    println("Happy Birthday, ${name}!")
    // Printing a cake
    println("   ,,,,,   ")
    println("   |||||   ")
    println(" ========= ")
    println("@@@@@@@@@@@")
    println("{~@~@~@~@~}")
    println("@@@@@@@@@@@")
    
    // Prints and empty line
    println("")
    
    println("You are already ${age}, ${name}!")
    println("${age} is the very best age to celebrate!")
}
